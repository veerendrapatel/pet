{
  "extends": ["airbnb", "plugin:import/errors", "plugin:jsx-a11y/recommended", "prettier", "prettier/react"],
  "plugins": ["import", "jsx-a11y", "react", "react-hooks"],
  "rules": {
    "no-console": 1,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react-hooks/rules-of-hooks": 2,
    "react-hooks/exhaustive-deps": 1
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
